# inventory/production - Production environment inventory
[all:vars]
ansible_user=ubuntu
ansible_python_interpreter=/usr/bin/python3

[webservers]
production-server ansible_host=YOUR_PRODUCTION_IP

[database]
production-server

[monitoring]
production-server

[k8s_master]
k8s-master ansible_host=YOUR_K8S_MASTER_IP

[k8s_workers]
k8s-worker-1 ansible_host=YOUR_K8S_WORKER1_IP
k8s-worker-2 ansible_host=YOUR_K8S_WORKER2_IP
