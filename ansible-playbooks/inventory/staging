# inventory/staging - Staging environment inventory
[all:vars]
ansible_user=ubuntu
ansible_python_interpreter=/usr/bin/python3

[webservers]
staging-server ansible_host=YOUR_STAGING_IP

[database]
staging-server

[monitoring]
staging-server

[k8s_master]
k8s-staging-master ansible_host=YOUR_K8S_STAGING_IP
